<h2>List of Bookings</h2>


<% @bookings.each do |booking| %>
   <p>Start: <%= booking.start%></p>
   <p>End: <%= booking.end%></p>
   <p>Purpose: <%= booking.purpose%></p>
   <p>Name:
   <%= link_to booking.room.name, booking.room%>
   </p>
<% end %>

<%= link_to "Create new Booking", new_booking_path%>
